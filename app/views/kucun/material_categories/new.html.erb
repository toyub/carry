<%= render 'form' %>


<% content_for :javascripts do %>
  <script>
  jQuery(function($){
    $('#new_store_material_category').on('submit', function(){
      $(this).ajaxSubmit(function(data, status, xhr){
        if(window.top != window.self){
          window.top.$$MIS.methods.get('add_root_category_callback').apply(data)
        }
      });
      return false;
    })
  });
  </script>
<% end %>
