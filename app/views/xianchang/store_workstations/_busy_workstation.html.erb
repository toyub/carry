<ul class="vehicle_info">
  <li class="float-left construction_info">
  <span><%= workstation.current_workflow.try(:name) %></span>
  <span><%= workstation.current_workflow.try(:store_vehicle).try(:current_license_number)%> </span>

  <%= link_to "加时", edit_xianchang_store_workflow_path(workstation.current_workflow), class: "add_time js-add-time", remote: true %>
  </li>
  <li class="float-left license_plate js-vehicle js-draggable" draggable="true" id="order-<%= workstation.current_workflow.store_order_id %>" data-order='{"previous_workstation": <%= workstation.id %>, "id": <%= workstation.current_workflow.store_order_id %>, "state": "processing"}'>
    <span class="do_dbclick <%= 'times' if is_started %>" data-countdown='<%= workstation.current_workflow.try(:ended_at) %>' data-stationid='<%=  workstation.id %>'></span>
  <img src="/images/car_front.png">
  </li>
</ul>
