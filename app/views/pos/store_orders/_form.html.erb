  <div class="content pos-orders-index"  id="content">
    <div class="main_top"><h2>业务开单</h2></div>
  <div class="details">
    <div class="search_nav">
       <div class="item-query">
        <label>查找车辆</label>
        <input type="text" class="width-180 query-license-number" placeholder="输入车牌">
        <a href="#" class="query_btn btn">查&nbsp;询</a>
      </div>
      <div class="query_div page_card">
      <a href="javascript:void(0)"  class="active">开单</a>
      <a  href="/pos/store_orders/">列表</a>
      </div>
    </div>

    <div class="billing">
      <div class="billing_order float-left">
        <div class="order_details">
          <div class="dialog_nav">
            <span class="dialog_title ">单号:20141105001</span>
          </div>
          <div class="vehicle_order_details">
            <%= render partial: 'vehicle' %>
            <%= render partial: 'vehicle_condition' %>
    
              <div class="vehicle_order">
                <span class="vehicle_order_title title doslide">
                  业务信息
                  <i class="fa-angle-double-up fa cursor-pointer"></i>
                  <a href="javascript:void(0)" data-target="order_items" class="do_show">选择</a>
                </span>
                <%= render partial: 'items' %>
              </div>
    
              <%= render partial: 'shigong' %>
              <%= render partial: 'others' %>
            </div>
            <div class="construction_status">
              <div class="btn_group">
                <a class="end_construction_btn">保存草稿</a>
                <a class="go_payment_btn">生成订单</a>
              </div>
            </div>
          </div>
        </div>

         <div class="right_information">
             <div class="order_items float-left store-order-item-block" data-id="order_items"></div>
            <%= render partial: 'construction' %>
            <div class="improve_crm_wrap float-left"></div>
            <%= render partial: 'vehicle_more' %>   
         </div>
      </div>
    
    </div>

</div>


