<div class="search_nav">
  <%= form_tag("/ais/costs", method: :get) do -%>
    <div class="item-query">
      <label>时间</label>
      <%= date_select(
        "", :date,
        { start_year: 2000, use_short_month: true, discard_day: true, :default => @date},
        {:include_blank => @search_date.nil?, class: "width-100" }
      )
    %>
    </div>

    <div class="item-query">
      <label>仓库选择</label>
      <select type="text" class="width-100">
        <option>全部</option>
      </select>

      <%= submit_tag "查询", class: "btn query_btn do_search_result" %>
    </div>

    <div class="query_div">
      <a href="#" class="export_btn btn">导出</a>
    </div>
  <% end %>
</div>
