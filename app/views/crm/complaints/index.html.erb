<%= render 'crm/common/page_head' %>
  <div class="details">
    <%= render '/crm/common/customer_nav' %>
    <%= search_form_for @q, url: crm_store_customer_complaints_path(@customer), class: 'search navbar-form' do |f| -%>
      <%= render 'crm/expense_records/search_form' %>
    <% end -%>
    <div class="details_content">
      <%= render 'list' %>
    </div>
  </div>

<% content_for :scenes do %>
  <div class="window_wrap" style="display: none;">
    <%= render 'dialog_form' %>
  </div>
<% end %>
