<% content_for :pre_assets do %>
  <%= stylesheet_link_tag "/stylesheets/kehu.css" %>
<% end %>

<%= render '/crm/common/page_head' %>

  <div class="details">
    <%= render '/crm/common/customer_nav' %>

    <div class="car_details float-right">

      <%= render '/crm/vehicle_common/vehicle_nav' %>

      <%= render '/crm/vehicle_common/sub_nav' %>

      <div class="details_content car_content">
          <div class="base_info">

            <%= render '/crm/vehicle_common/vehicle_image' %>

            <div class="car_info">
              <div class="items">
                <%= form_for @vehicle, url: crm_store_customer_store_vehicles_path do |f| %>
                  <%= render 'edit_form', f: f %>
                  <%= render 'form', f: f %>
                  <div class="btn_group">
                    <%= f.submit '创建', class: 'save_btn btn' %>
                  </div>
                <% end %>
              </div>
            </div>
          </div>
      </div>
    </div>
  </div>
  <script type="text/javascript">
    $(function(){
      $('.datepicker').datetimepicker({
        format: 'Y-m-d',
        lang: 'ch',
        timepicker: false
      });

      $(document).on('click', '#brand', xx);
    });

    function xx(){
      alert('x')
    }
  </script>
